:: StoryScript [script]
jQuery(document).ready(function () {

    // Inicialitza els comptadors de clics per a hotspots si no existeixen
    if (typeof State !== 'undefined' && typeof State.variables !== 'undefined') {
        if (State.variables.hotspotClicks === undefined) {
            State.variables.hotspotClicks = {};
        }
        // NOU: Inicialitza la variable de transici√≥ temporal
        if (State.variables.tempPassageTransition === undefined) {
            State.variables.tempPassageTransition = "";
        }
    } else {
        console.error("ERROR: State.variables no est√† definit. Els comptadors de hotspot i les transicions no funcionaran.");
    }


    // Funci√≥ auxiliar per avaluar les condicions
    function evaluateCondition(conditionVarName, conditionVal) {
        if (typeof State === 'undefined' || typeof State.variables === 'undefined') {
            console.error("ERROR: State.variables no est√† definit per avaluar la condici√≥.");
            return false;
        }
        if (!conditionVarName || conditionVal === undefined || conditionVal === null) {
            return true; 
        }

        const varName = conditionVarName.substring(1); 
        const currentValue = State.variables[varName];
        let expression = ''; 
        
        conditionVal = String(conditionVal); 

        let operatorUsed = '';
        let valuePart = conditionVal;

        const operators = ['===', '==', '!==', '!=', '>=', '<=', '>', '<'];
        for (const op of operators) {
            if (conditionVal.startsWith(op)) {
                operatorUsed = op;
                valuePart = conditionVal.substring(op.length).trim();
                break;
            }
        }

        if (!operatorUsed) { 
            operatorUsed = '==';
        }
        
        let parsedValuePart;
        // Try to parse as boolean first
        if (valuePart === 'true') parsedValuePart = true;
        else if (valuePart === 'false') parsedValuePart = false;
        // Then try to parse as number
        else if (!isNaN(Number(valuePart)) && valuePart !== '') parsedValuePart = Number(valuePart);
        // Otherwise, keep as string (and ensure it's quoted for eval)
        else parsedValuePart = valuePart; 
        
        expression = `${JSON.stringify(currentValue)} ${operatorUsed} ${JSON.stringify(parsedValuePart)}`;

        try {
            const result = eval(expression); 
            return result;
        } catch (e) {
            console.error("ERROR: Error avaluant la condici√≥:", expression, e);
            return false;
        }
    }

    // Funci√≥ auxiliar per reproduir un so amb HAL
    function playHotspotSound(soundId) {
        if (soundId && typeof Chapel !== 'undefined' && typeof Chapel.Audio !== 'undefined' && typeof Chapel.Audio.track === 'function') {
            try {
                const track = Chapel.Audio.track(soundId);
                track.stop(); 
                track.playWhenPossible();
                // console.log("DEBUG: üîä So reprodu√Øt via HAL:", soundId); 
            } catch (e) {
                console.error("ERROR: Error en reproduir el so amb HAL:", soundId, e.message);
            }
        } else if (soundId) {
            console.warn("ADVERTENCIA: L'API de HAL no est√† disponible per al so:", soundId);
        }
    }

    // Funci√≥ auxiliar per aturar un so amb HAL
    function stopHotspotSound(soundId) {
        if (soundId && typeof Chapel !== 'undefined' && typeof Chapel.Audio !== 'undefined' && typeof Chapel.Audio.track === 'function') {
            try {
                const track = Chapel.Audio.track(soundId);
                track.stop(); 
            } catch (e) {
                // console.warn("ADVERTENCIA: No s'ha pogut aturar el so amb HAL:", soundId, e.message); 
            }
        }
    }
    
    // Funci√≥ per aturar tots els sons de hotspots actius (√∫til en canvis de passatge)
    function stopAllHotspotSounds() {
        jQuery('.hotspot-item').each(function() {
            const soundId = jQuery(this).data('sound');
            const soundFailId = jQuery(this).data('sound-fail');
            const soundClickId = jQuery(this).data('sound-click');
            const soundClickFailId = jQuery(this).data('sound-click-fail');

            stopHotspotSound(soundId);
            stopHotspotSound(soundFailId);
            stopHotspotSound(soundClickId);
            stopHotspotSound(soundClickFailId);
        });
    }


    var TWINE_IMAGE_INFO_DISPLAY_ID = 'twineImageInfoDisplay'; 

    // Funci√≥ per assignar el text a $tempInfoDisplay i fer que Harlowe el processi
    function toggleImageInfo(text) {
        console.log("DEBUG: toggleImageInfo cridada amb text:", text);
        if (typeof State !== 'undefined' && State.variables) {
            State.variables.tempInfoDisplay = text; 
            console.log("DEBUG: $tempInfoDisplay set a:", State.variables.tempInfoDisplay);
            
            if (text === undefined || text === null || String(text).trim() === '') {
                 jQuery('#' + TWINE_IMAGE_INFO_DISPLAY_ID).hide();
                 console.log("DEBUG: #twineImageInfoDisplay amagat (text buit).");
            } else {
                jQuery('#' + TWINE_IMAGE_INFO_DISPLAY_ID).show();
                console.log("DEBUG: #twineImageInfoDisplay mostrat.");
            }
        } else {
            console.error("ERROR: State.variables no est√† definit. No es pot actualitzar $tempInfoDisplay.");
        }
    }


    // Funci√≥ per executar les operacions de setter d'un hotspot
    function executeHotspotSetters(hotspotElement) {
        const setterRaw = jQuery(hotspotElement).get(0).dataset.setter;
        const dataVariable2 = jQuery(hotspotElement).data('variable-2');
        const dataValor2 = jQuery(hotspotElement).data('valor-2');

        let combinedSetterStatements = setterRaw || '';

        if (dataVariable2 && dataValor2) {
            const secondSetterStatement = `${dataVariable2} ${dataValor2}`;
            console.log("DEBUG: Second Setter Statement Constructed:", secondSetterStatement);
            if (combinedSetterStatements) {
                combinedSetterStatements += `; ${secondSetterStatement}`;
            } else {
                combinedSetterStatements = secondSetterStatement;
            }
            console.log("DEBUG: Combined Setter Statements for execution:", combinedSetterStatements);
        }

        if (!combinedSetterStatements || typeof State === 'undefined' || typeof State.variables === 'undefined') return;
        const vars = State.variables;

        const statements = combinedSetterStatements.split(';');
        statements.forEach(stmt => {
            const trimmedStmt = stmt.trim();
            if (!trimmedStmt) return;
            const match = trimmedStmt.match(/^\s*(\$\w+)\s*(=|\+=|-=|\*=|\/=)\s*(.*)\s*$/);
            if (!match) {
                console.warn("ADVERTENCIA: Setter no reconegut o mal formatat:", trimmedStmt);
                return;
            }

            const varName = match[1].substring(1);
            const operator = match[2];
            let valueStr = match[3];

            let value;
            if (!isNaN(Number(valueStr))) {
                value = Number(valueStr);
            } else if (valueStr === 'true') {
                value = true;
            } else if (valueStr === 'false') {
                value = false;
            } else if (valueStr.startsWith('$')) {
                const targetVarName = valueStr.substring(1);
                value = vars[targetVarName];
            } else {
                value = valueStr.replace(/^['"]|['"]$/g, '');
            }

            if (vars[varName] === undefined) {
                vars[varName] = (typeof value === 'number') ? 0 : "";
            }

            switch (operator) {
                case '=':
                    vars[varName] = value;
                    break;
                case '+=':
                    if (typeof value === 'number' && typeof vars[varName] === 'number') vars[varName] += value;
                    else vars[varName] += String(value);
                    break;
                case '-=':
                    if (typeof value === 'number' && typeof vars[varName] === 'number') vars[varName] -= value;
                    else console.warn(`ADVERTENCIA: Intent de restar un valor no num√®ric de ${varName} o ${value}.`);
                    break;
                case '*=':
                    if (typeof value === 'number' && typeof vars[varName] === 'number') vars[varName] *= value;
                    else console.warn(`ADVERTENCIA: Intent de multiplicar amb un valor no num√®ric de ${varName} o ${value}.`);
                    break;
                case '/=':
                    if (typeof value === 'number' && typeof vars[varName] === 'number') {
                        if (value !== 0) vars[varName] /= value;
                        else console.error(`ERROR: Divisi√≥ per zero en ${varName} /= ${value}.`);
                    } else {
                        console.warn(`ADVERTENCIA: Intent de dividir amb un valor no num√®ric de ${varName} o ${value}.`);
                    }
                    break;
                default:
                    console.warn("ADVERTENCIA: Operador no suportat pel setter:", operator);
            }
        });
    }

    // Gesti√≥ de 'mouseenter' per mostrar informaci√≥ i reproduir so
    jQuery(document).on('mouseenter', '.hotspot-item', function () { 
        const hotspotId = jQuery(this).attr('alt'); // Utilitzem l'alt com a ID √∫nic
        const visitLimit = parseInt(jQuery(this).data('visit-limit'));
        const limitVisitEnter = jQuery(this).attr('data-visit-limit-enter'); 
        const soundFailId = jQuery(this).data('sound-fail'); 

        // Comprovem si el hotspot t√© un l√≠mit de visites i si aquest s'ha superat
        if (!isNaN(visitLimit) && State.variables.hotspotClicks[hotspotId] >= visitLimit) {
            console.log(`DEBUG: Mouseenter - L√≠mit de visites (${hotspotId}) assolit: ${State.variables.hotspotClicks[hotspotId]}/${visitLimit}. Text d'entrada:`, limitVisitEnter);
            toggleImageInfo(limitVisitEnter);
            playHotspotSound(soundFailId); 
            return; 
        }

        const conditionVarName = jQuery(this).data('condition-var');
        const conditionVal = jQuery(this).data('condition-val');
        const infoText = jQuery(this).attr('data_info');
        const failEnterInfo = jQuery(this).attr('text_fail_enter');
        const soundId = jQuery(this).data('sound'); 

        const isConditionMet = evaluateCondition(conditionVarName, conditionVal);

        if (isConditionMet) {
            if (infoText) {
                toggleImageInfo(infoText);
            } else {
                toggleImageInfo(''); 
            }
            playHotspotSound(soundId); // So normal de mouseenter
        } else {
            if (failEnterInfo) {
                toggleImageInfo(failEnterInfo);
            } else {
                toggleImageInfo(''); 
            }
            playHotspotSound(soundFailId); // So de fallada de mouseenter
        }
    });

    // Gesti√≥ de 'mouseleave' per amagar informaci√≥ i aturar sons (nom√©s sons d'hover)
    jQuery(document).on('mouseleave', '.hotspot-item', function () { 
        toggleImageInfo(''); 

        const soundId = jQuery(this).data('sound');
        const soundFailId = jQuery(this).data('sound-fail');

        // Atura nom√©s els sons de mouseenter
        if (soundId) { 
            stopHotspotSound(soundId);
        }
        if (soundFailId) { 
            stopHotspotSound(soundFailId);
        }
    });


    // Gesti√≥ de clics en hotspots amb data-passage
    jQuery(document).on('click', 'area.hotspot-item[data-passage]', function () {
        const hotspotId = jQuery(this).attr('alt'); // Utilitzem l'alt com a ID √∫nic
        const visitLimit = parseInt(jQuery(this).data('visit-limit'));
        const limitVisitClic = jQuery(this).attr('data-visit-limit-clic'); 
        
        const soundId = jQuery(this).data('sound'); // So de mouseenter exit√≥s
        const soundFailId = jQuery(this).data('sound-fail'); // So de mouseenter fallat
        const soundClickId = jQuery(this).data('sound-click'); // NOU: So de clic exit√≥s
        const soundClickFailId = jQuery(this).data('sound-click-fail'); // NOU: So de clic fallat
        const transitionArrive = jQuery(this).data('transition-arrive'); // NOU: Transici√≥ d'arribada

        // ATURA ELS SONS DE HOVER (mouse enter/fail) abans de qualsevol altra cosa en aquest hotspot
        stopHotspotSound(soundId);
        stopHotspotSound(soundFailId);

        // Comprovem si el hotspot t√© un l√≠mit de visites i si aquest s'ha superat
        if (!isNaN(visitLimit) && State.variables.hotspotClicks[hotspotId] >= visitLimit) {
            console.log(`DEBUG: Clic - L√≠mit de visites (${hotspotId}) assolit: ${State.variables.hotspotClicks[hotspotId]}/${visitLimit}. Text de clic:`, limitVisitClic, ". No es navega.");
            toggleImageInfo(limitVisitClic);
            // Si hi ha so de clic fallat, sona. Si no, no sona res.
            if (soundClickFailId) { 
                playHotspotSound(soundClickFailId); 
            }
            return; 
        }

        const passageName = jQuery(this).data('passage');
        const conditionVarName = jQuery(this).data('condition-var');
        const conditionVal = jQuery(this).data('condition-val');
        const failClickInfo = jQuery(this).attr('text_fail_clic');
        

        if (passageName && typeof State !== 'undefined' && State.variables) {
            const isConditionMet = evaluateCondition(conditionVarName, conditionVal);

            if (isConditionMet) {
                // Executem els setters abans de la navegaci√≥ si la condici√≥ es compleix
                executeHotspotSetters(this); // 'this' es refereix a l'element 'area' clicat

                if (!isNaN(visitLimit)) { 
                    State.variables.hotspotClicks[hotspotId] = (State.variables.hotspotClicks[hotspotId] || 0) + 1;
                }
                // Si hi ha so de clic exit√≥s, sona. Si no, no sona res.
                if (soundClickId) {
                    playHotspotSound(soundClickId); 
                }
                
                // NOU: Estableix la transici√≥ d'arribada abans de navegar
                State.variables.tempPassageTransition = transitionArrive || ""; 

                State.variables.retornA = passageName; 
                const link = jQuery("tw-link").filter(function () {
                    const text = jQuery(this).text().trim();
                    return text.toLowerCase() === passageName.trim().toLowerCase();
                });
                if (link.length > 0) {
                    link.click(); 
                } else {
                    console.warn("ADVERTENCIA: No s'ha trobat <tw-link> per a la navegaci√≥ Harlowe:", passageName);
                }
            } else {
                // Si hi ha so de clic fallat, sona. Si no, no sona res.
                if (soundClickFailId) {
                    playHotspotSound(soundClickFailId);
                }
                if (failClickInfo) {
                    toggleImageInfo(failClickInfo); 
                } else {
                    toggleImageInfo(''); 
                }
            }
        }
    });

    // Gesti√≥ de clics en hotspots amb data-setter
    jQuery(document).on('click', 'area.hotspot-item[data-setter]', function () {
        const hotspotId = jQuery(this).attr('alt'); // Utilitzem l'alt com a ID √∫nic
        const visitLimit = parseInt(jQuery(this).data('visit-limit'));
        const limitVisitClic = jQuery(this).attr('data-visit-limit-clic'); 
        
        const soundId = jQuery(this).data('sound'); // So de mouseenter exit√≥s
        const soundFailId = jQuery(this).data('sound-fail'); // So de mouseenter fallat
        const soundClickId = jQuery(this).data('sound-click'); // NOU: So de clic exit√≥s
        const soundClickFailId = jQuery(this).data('sound-click-fail'); // NOU: So de clic fallat

        // ATURA ELS SONS DE HOVER (mouse enter/fail) abans de qualsevol altra cosa en aquest hotspot
        stopHotspotSound(soundId);
        stopHotspotSound(soundFailId);

        // Comprovem si el hotspot t√© un l√≠mit de visites i si aquest s'ha superat
        if (!isNaN(visitLimit) && State.variables.hotspotClicks[hotspotId] >= visitLimit) {
            console.log(`DEBUG: Clic - L√≠mit de visites (${hotspotId}) assolit: ${State.variables.hotspotClicks[hotspotId]}/${visitLimit}. Text de clic:`, limitVisitClic, ". No s'executa setter.");
            toggleImageInfo(limitVisitClic);
            // Si hi ha so de clic fallat, sona. Si no, no sona res.
            if (soundClickFailId) {
                playHotspotSound(soundClickFailId); 
            }
            return; 
        }

        const conditionVarName = jQuery(this).data('condition-var');
        const conditionVal = jQuery(this).data('condition-val');
        const failClickInfo = jQuery(this).attr('text_fail_clic'); 
        const infoText2 = jQuery(this).attr('data_info_2');
        const infoText = jQuery(this).attr('data_info'); 
        

        const isConditionMet = evaluateCondition(conditionVarName, conditionVal);

        if (isConditionMet) {
            // Executem els setters si la condici√≥ es compleix
            executeHotspotSetters(this); // 'this' es refereix a l'element 'area' clicat

            if (!isNaN(visitLimit)) { 
                State.variables.hotspotClicks[hotspotId] = (State.variables.hotspotClicks[hotspotId] || 0) + 1;
            }

            // Si hi ha so de clic exit√≥s, sona. Si no, no sona res.
            if (soundClickId) {
                playHotspotSound(soundClickId); 
            }
            // El display d'informaci√≥ s'actualitza despr√©s d'executar els setters, per si les variables canvien els textos
            if (infoText2) {
                 toggleImageInfo(infoText2);
            } else {
                toggleImageInfo(infoText);
            }
        } else {
            // Si hi ha so de clic fallat, sona. Si no, no sona res.
            if (soundClickFailId) {
                playHotspotSound(soundClickFailId);
            }
            if (failClickInfo) {
                toggleImageInfo(failClickInfo); 
            } else {
                toggleImageInfo('');
            }
        }
    });

    // Esdeveniments de passatge
    // NOU: Atura tots els sons dels hotspots i neteja la variable de transici√≥ quan un passatge s'inicia o es mostra
    jQuery(document).on(':passagedisplay', function() {
        stopAllHotspotSounds(); // Atura tots els sons en el display del passatge
        if (typeof State !== 'undefined' && State.variables) {
            State.variables.tempInfoDisplay = '';
        }
        jQuery('#' + TWINE_IMAGE_INFO_DISPLAY_ID).empty().hide();
    });

    jQuery(document).on(':passageinit', function() {
        stopAllHotspotSounds(); // Atura tots els sons en la inicialitzaci√≥ del passatge (aquesta √©s m√©s robusta)
        if (typeof State !== 'undefined' && State.variables) {
            State.variables.tempInfoDisplay = '';
            // NOU: Neteja la variable de transici√≥ en la inicialitzaci√≥ del passatge
            // Aquesta √©s una segona l√≠nia de defensa per si la de Harlowe no es crida per alguna ra√≥.
            State.variables.tempPassageTransition = ""; 
        }
        jQuery('#' + TWINE_IMAGE_INFO_DISPLAY_ID).empty().hide(); 
    });
});



